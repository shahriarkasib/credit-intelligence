# Credit Intelligence Environment Variables
# Copy this file to .env and fill in your API keys

# LangSmith (for tracing) - https://smith.langchain.com
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=credit-intelligence
LANGCHAIN_API_KEY=your_langsmith_api_key

# Finnhub (stock data) - https://finnhub.io
FINNHUB_API_KEY=your_finnhub_api_key

# CourtListener (legal data) - https://www.courtlistener.com
COURTLISTENER_API_KEY=your_courtlistener_api_key

# SEC EDGAR (required, no key needed)
SEC_EDGAR_USER_AGENT=YourName contact@example.com

# Groq LLM (FREE) - https://console.groq.com
GROQ_API_KEY=your_groq_api_key

# MongoDB (cloud storage) - https://cloud.mongodb.com
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/credit_intelligence

# Optional
OPENCORPORATES_API_KEY=
